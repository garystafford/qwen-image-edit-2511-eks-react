# Qwen Image Edit - EKS Deployment Configuration
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# AWS Configuration
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1

# EKS Cluster
EKS_CLUSTER_NAME=my-eks-cluster
EKS_NODEGROUP_NAME=gpu-nodes

# Kubernetes
K8S_NAMESPACE=qwen
K8S_SERVICE_ACCOUNT=qwen-s3-access

# ECR Repository Names
ECR_REPO_UI=qwen-ui
ECR_REPO_MODEL=qwen-model

# ALB Ingress Controller
ALB_CONTROLLER_VERSION=v2.9.2

# CloudFront + Cognito Authentication (optional)
# Used by scripts/setup-cloudfront-auth.sh
COGNITO_USER_POOL_ID=us-east-1_EXAMPLE
APP_DOMAIN=qwen.example.com
ALB_NAME=qwen-alb

# Model Configuration
# Used by scripts/upload-weights-to-s3.py
MODEL_ID=ovedrive/Qwen-Image-Edit-2511-4bit
S3_PREFIX=qwen-image-edit-2511-4bit
